+#include "DependencyManager.h"
+#include <unordered_set>
+#include <vector>
+
+std::vector<std::string> getTopologicalOrder(const std::unordered_map<std::string, std::set<std::string>>& graph) {
+  std::unordered_set<std::string> visited;
+  std::vector<std::string> sortedFiles;
+
+  auto performTopologicalSort = [&](const std::string& node, auto&& performTopologicalSort) -> void {
+    if (!visited.insert(node).second) return;
+
+    if (graph.count(node)) {
+      for (const auto& neighbor : graph.at(node)) {
+        performTopologicalSort(neighbor, performTopologicalSort);
+      }
+    }
+
+    sortedFiles.push_back(node);
+  };
+
+  for (const auto& [file, _] : graph) {
+    if (!visited.count(file)) {
+      performTopologicalSort(file, performTopologicalSort);
+    }
+  }
+
+  return sortedFiles;
+}
+
+std::vector<std::string> DependencyManager::getOrderedFiles() const {
+  return getTopologicalOrder(dependencyGraph);
