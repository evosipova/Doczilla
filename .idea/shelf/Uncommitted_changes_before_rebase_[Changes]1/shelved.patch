@@ -0,0 +1,22 @@
+#include <string>
+#include <unordered_map>
+#include <set>
+#include <vector>
+
+class DependencyManager {
+private:
+  std::unordered_map<std::string, std::set<std::string>> dependencyGraph;
+
+public:
+  void registerFile(const std::string& fileName) {
+    if (dependencyGraph.find(fileName) == dependencyGraph.end()) {
+      dependencyGraph[fileName] = {};
+    }
+  }
+
+  void addDependency(const std::string& from, const std::string& to) {
+    dependencyGraph[from].insert(to);
+  }
+
+  std::vector<std::string> getOrderedFiles() const;
+};
